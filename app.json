{
	"expo": {
		"name": "Car Calendar",
		"slug": "carcalendar",
		"platforms": ["android", "ios"],
		"version": "v1.2",
		"orientation": "portrait",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"android": {
			"package": "com.ashnwor.carcalendar",
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/ic_launcher_foreground.png",
				"backgroundImage": "./assets/ic_launcher_background.png"
			}
		},
		"ios": {
			"bundleIdentifier": "com.ashnwor.carcalendar",
			"buildNumber": "1.0.0",
			"icon": "./assets/ic_launcher_ios.png"
		},
		"description": "A React Native app to manage rental cars.",
		"githubUrl": "https://github.com/Ashnwor/rn-car-calendar"
	}
}
